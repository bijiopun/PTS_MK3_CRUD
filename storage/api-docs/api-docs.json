                "summary": "Ambil semua laporan pengiriman",
                "operationId": "b5c47725fa95f25f1b449422c9afb5e6",
                "responses": {
                    "200": {
                        "description": "Data laporan berhasil diambil"
                    }
                }
            },
            "post": {
                "tags": [
                    "LaporanPengiriman"
                ],
                "summary": "Buat laporan pengiriman baru",
                "operationId": "48d0a2bde5552917ec3417e13a689a36",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "wilayah",
                                    "jumlah_paket",
                                    "status_pengiriman"
                                ],
                                "properties": {
                                    "wilayah": {
                                        "type": "string"
                                    },
                                    "jumlah_paket": {
                                        "type": "integer"
                                    },
                                    "status_pengiriman": {
                                        "type": "string",
                                        "enum": [
                                            "pending",
                                            "dalam perjalanan",
                                            "selesai"
                                        ]
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Laporan berhasil dibuat"
                    }
                }
            }
        },
